@model IEnumerable<ZJUCoursera1.DAL.SC>

@{
    ViewBag.Title = "我的课程";
}

<h2>已选课程</h2>

<p>
    @Html.ActionLink("添加选课", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SelectAt)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Course.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Student.Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SelectAt)
        </td>

    </tr>
}

</table>
